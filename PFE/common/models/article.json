{
  "name": "Article",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nom": {
      "type": "string",
      "required": true
    },
    "prix": {
      "type": "number",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "gamme": {
      "type": "belongsTo",
      "model": "Gamme",
      "foreignKey": "GammeId"
    },
    "categorie": {
      "type": "belongsTo",
      "model": "Categorie",
      "foreignKey": "CategorieId"
    },
    "marque": {
      "type": "belongsTo",
      "model": "Marque",
      "foreignKey": "MarqueId"
    },
    "image": {
      "type": "belongsTo",
      "model": "Image",
      "foreignKey": "ImageId"
    },
    "paniers": {
      "type": "hasMany",
      "model": "Panier",
      "foreignKey": "PanierId",
      "through": "Article_Panier"
    }
  },
  "acls": [],
  "methods": {}
}
