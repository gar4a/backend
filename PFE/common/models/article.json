{
  "name": "Article",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "das": {
      "type": "number",
      "required": true
    },
    "images": {
      "type": "array",
      "required": true
    },
    "description": {
      "type": "array",
      "required": true
    },
    "stockage": {
      "type": "array",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "gamme": {
      "type": "belongsTo",
      "model": "Gamme",
      "foreignKey": "gammeId",
      "options": {
        "nestRemoting": true
      }
    },
    "categorie": {
      "type": "belongsTo",
      "model": "Categorie",
      "foreignKey": "categorieId",
      "options": {
        "nestRemoting": true
      }
    },
    "marque": {
      "type": "belongsTo",
      "model": "Marque",
      "foreignKey": "marqueId",
      "options": {
        "nestRemoting": true
      }
    },
    "commandes": {
      "type": "hasAndBelongsToMany",
      "model": "Commande",
      "foreignKey": "articleId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
